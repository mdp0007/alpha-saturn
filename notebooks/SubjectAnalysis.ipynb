        "import spacy\n",
        "nlp = spacy.load('en_core_web_sm')\n",
        "sent = \"Supernus to Acquire CNS Portfolio from US WorldMeds\"\n",
        "doc=nlp(sent)\n",
        "\n",
        "sub_toks = [tok for tok in doc if (tok.dep_ == \"nsubj\") ]\n",
        "\n",
        "print(sub_toks) \n",
        "print([(w.text, w.pos_) for w in doc])\n",
        "print([(w.text, w.dep_) for w in doc])"
